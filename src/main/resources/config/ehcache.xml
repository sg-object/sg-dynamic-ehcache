<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="false"
    monitoring="autodetect" dynamicConfig="true">
 
    <diskStore path="C:/cache_test" />

	<defaultCache maxElementsInMemory="10000" eternal="true"
		overflowToDisk="true" maxEntriesLocalHeap="10000" diskPersistent="true">
	</defaultCache>
	
    <cache name="cacheNames" maxElementsInMemory="10000"
		eternal="true" overflowToDisk="true" maxEntriesLocalHeap="10000"
		diskPersistent="true">
	</cache>
	
</ehcache>